# .bashrc_aliases
# Project: .prompt
# Description: Custom bash aliases
# Creator: Justin Doyle <justin@jmdoyle.com>
# Date: 12/16/2015

## Native Client Build
export NACL_SDK_ROOT=/var/www/native_client/nacl_sdk/pepper_26
export NACLVERBOSITY=1
export NACL_PLUGIN_DEBUG=1
export NACL_DEBUG_ENABLE=1
export PPAPI_BROWSER_DEBUG=1
export NACL_SRPC_DEBUG=255

## MySQL
export PATH=$PATH:/opt/local/lib/mysql55/bin
export PATH=$PATH:/opt/local/bin:/opt/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/usr/local/git/bin:/usr/bin/phpstorm/bin:/opt/local/lib/mysql55/bin

## Android
export PATH=$PATH:/Users/jdoyle/android-sdk/tools
export ANDROID_HOME=$HOME/android-sdk

## Hashicorp
export PATH=$PATH:/usr/local/bin/terraform/
export PATH=$PATH:/home/jdoyle/packer

## AWS
export PATH=/usr/local/aws/bin:$PATH

## AWS Command Completion
complete -C '/usr/local/bin/aws_completer' aws

## Misc Aliases


## Swagger
alias sp="swagger project"
alias sps="swagger project start"
alias spe="swagger project edit"
